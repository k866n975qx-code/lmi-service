# Lunch Money
LM_TOKEN=your_lm_token_here
LM_BASE_URL=https://dev.lunchmoney.app
LM_PLAID_ACCOUNT_IDS=
LM_START_DATE=2025-10-01
LM_LOOKBACK_DAYS=7

# Providers (disable by setting to 0)
YF_ENABLE=1
NASDAQ_ENABLE=1
PROVIDERS_OPENBB=1
YQ_ENABLE=1
STOOQ_ENABLE=1
FRED_API_KEY=

# Runtime
LOCAL_TZ=America/Los_Angeles
DAILY_CUTOVER=00:00
DB_PATH=./data/app.db

# Alerts / Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
ALERTS_DAILY_HOUR=7
ALERTS_DAILY_MINUTE=30
ALERTS_WEEKLY_ENABLED=1
ALERTS_MONTHLY_ENABLED=1
ALERTS_QUARTERLY_ENABLED=1

# CUSIP mapping
CUSIP_CSV=./CUSIP.csv

# Benchmarks
BENCHMARK_PRIMARY=^GSPC
BENCHMARK_SECONDARY=SPY

# Sync + rate limits
SYNC_TIME_BUDGET_SECONDS=3600
HTTP_TIMEOUT_SECONDS=30
HTTP_RETRY_ATTEMPTS=3
HTTP_RETRY_BACKOFF_SECONDS=1
MARKET_BATCH_SIZE=25
MARKET_RATE_LIMIT_SECONDS=0.2
MARKET_RETRY_ATTEMPTS=2

# Cache
CACHE_ENABLED=1
CACHE_DIR=./.cache
CACHE_DB_PATH=./data/cache.sqlite3
CACHE_TTL_HOURS=24

# Logging
LOG_LEVEL=INFO
LOG_ERROR_FILE=./data/logs/error.log
LOG_VERBOSE=1

# Goal & margin guidance (optional)
GOAL_TARGET_MONTHLY=2000
GOAL_MONTHLY_CONTRIBUTION=3000
GOAL_GROWTH_WINDOW_MONTHS=3
MARGIN_APR_CURRENT=0.0415
MARGIN_APR_FUTURE=0.0565
MARGIN_APR_FUTURE_DATE=2026-11-01
